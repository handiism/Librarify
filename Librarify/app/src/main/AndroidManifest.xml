<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.handira.librarify" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ProyekMagang" >
        <activity
            android:name="com.handira.librarify.activity.EditMember"
            android:label="@string/title_activity_edit_member"
            android:theme="@style/Theme.ProyekMagang.NoActionBar" >
        </activity>
        <activity android:name="com.handira.librarify.activity.ViewMember" />
        <activity android:name="com.handira.librarify.activity.AddMember" />
        <activity android:name="com.handira.librarify.activity.Main" />
        <activity android:name="com.handira.librarify.activity.Splash" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.handira.librarify.activity.Login" />
    </application>

</manifest>